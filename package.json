{
  "author": "robertsmieja",
  "bin": {
    "ric": "./bin/run",
    "run-in-container": "./bin/run"
  },
  "bugs": "https://github.com/robertsmieja/run-in-container/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.3",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "conf": "^7.1.2",
    "conf-cli": "^0.1.9",
    "inquirer": "^7.3.3",
    "json-schema-typed": "^7.0.3",
    "lookpath": "^1.1.0",
    "tslib": "^2.0.1"
  },
  "description": "CLI to assist running commands inside containers",
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "ajv": "^6.12.5",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/robertsmieja/run-in-container",
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "oclif",
    "run",
    "in",
    "container",
    "ric",
    "run-in-container"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "lib/index.js",
  "name": "run-in-container",
  "oclif": {
    "bin": "run-in-container",
    "commands": "./lib/commands",
    "hooks": {
      "init": [
        "./lib/hooks/init"
      ]
    },
    "plugins": [
      "conf-cli",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "robertsmieja/run-in-container",
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint --fix ./src/**/*",
    "lint:staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --fix",
    "prepack": "npm-run-all test clean build && oclif-dev manifest && oclif-dev readme && git add README.md",
    "test": "jest --coverage",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "version": "0.2.2"
}
